{
    "name": "dinandmentink/nova-markdown",
    "description": "A Laravel Nova Markdown Field.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "markdown"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^8.13"
    },
    "autoload": {
        "psr-4": {
            "DinandMentink\\Markdown\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DinandMentink\\Markdown\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
